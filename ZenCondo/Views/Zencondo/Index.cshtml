    function registermodel() {
        $('#RegisterModal').modal('show');
    }

    function firstreg(first)
    {
        common(first);
    }

    function secondreg(secd) {

        common2(secd);
    }

    function common(txt) {

        debugger;
        var chk = $("#confrmation").prop('checked');
        var chk2 = $("#confrmation2").prop('checked');


        if (txt == "popupreg")
        {
            if ($("#confrmation").prop('checked') != true) {
                $('#display_txt').text('');
                $('#display_txt').text('Must select the checkbox.');
                $('#myModal').modal('show');
                return false;
            }
        }
        else if (txt == "reg")
        {
            if ($("#confrmation2").prop('checked') != true) {
                $('#display_txt').text('');
                $('#display_txt').text('Must select the checkbox.');
                $('#myModal').modal('show');
                return false;
            }
        }



        if (ValidUsername() && ValidEmail() && ValidPhone() && RealtorChecked()) {
            var person = {
                name: $("#userName").val(),
                email: $("#userEmail").val(),
                phone: $("#userPhone").val(),
                IsRealtor: $("#IsReator option:selected").text(),

            }

            $('.container').addClass('onblur');

            $.ajax({
                url: '/Zencondo/sendmail',
                type: 'post',
                dataType: 'json',
                success: function (data) {
                    $('#target').html(data.msg);
                    $('#display_txt').text('');
                    $('#display_txt').text('mail send successfully.');
                    $('#myModal').modal('show');
                    $('.container').removeClass('onblur');
                    $("#userName").val(''),
                    $("#userEmail").val(''),
                    $("#userPhone").val(''),
                    $("#userSubject").val(''),
                    $("#Message").val('')
                },
                data: person
            });
        }
        else {
            return false;
        }
    }

